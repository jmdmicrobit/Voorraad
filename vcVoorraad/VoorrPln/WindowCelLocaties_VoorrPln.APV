PROPERTY:BEGIN
PRIORITY 4500
PROPERTY:END
IF GlobalRequest=DeleteRecord
	IF Ver2:Verwerkt THEN
		!MESSAGE('Verkoop is verwerkt en kan niet worden verwijderd.')
		!RETURN ReturnValue
	.
	
	Verwerkt# = false
	NoRecord# = true
	Pla:VerkoopID = Ver2:VerkoopID
	CLEAR(Pla:PlanningID,-1)
	SET(Pla:Planning_FK02,Pla:Planning_FK02)
