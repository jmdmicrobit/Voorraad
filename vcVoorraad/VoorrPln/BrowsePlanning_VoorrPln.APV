	db.DebugOut('INSLAG DOEN2a ' & APla:PartijID)
	GlobalRequest = ChangeRecord
	
	IF(APla:Gewogen)
		WindowInslagWeging()
	ELSE
		WindowInslag()
	END

    IF GlobalResponse = RequestCompleted
        Clear(APla:REcord)
        APla:PlanningID = BRW6.Q.Pla2:PlanningID
