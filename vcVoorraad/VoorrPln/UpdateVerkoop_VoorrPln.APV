    UNHIDE(?BtnRetour)
    IF Ver2:RedenRetour = ''
        ?BtnRetour{Prop:Text}='Retour boeken:'
    ELSE
    !    DISABLE(?BtnRetour)
        ?BtnRetour{Prop:Text}='Retour reden:'&Ver2:RedenRetour&' '&Format(Ver2:RetourDatum_DATE,@d5-)
