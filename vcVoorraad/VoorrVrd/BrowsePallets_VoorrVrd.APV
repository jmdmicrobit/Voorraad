          LIST,AT(13,63,429,182),USE(?Browse:1),HVSCROLL,FORMAT('160L(2)|M~Firmanaam~C(' & |
            '0)@s50@50C|MI~Transporteur~C(0)@p p@60L(2)|M~Palletsoort~C(0)@s20@[45R(2)|' & |
            'M@d6-@40R(2)|M@t1@](73)|~Datum~[54R(2)|M~IN~C(0)@n-14.@51R(2)|M~UIT~C(0)@n' & |
            '-14.@56R(2)|M~Saldo*~C(0)@n-14.@](158)|~ ~55R(2)|~IN (Klant)~C(1)@n-14.@80' & |
            'L(2)|M~Klant~C(0)@s50@40L(2)|I~Controle~C(0)@PP@45L(2)|I~Handmatig~C(0)@PP' & |
            '@100L(2)|~Omschrijving~C(0)@s50@0L(2)~Pallet Mutatie ID~C(0)@n-14@0L(-50)|M@n-14@'), |
            FROM(Queue:Browse:1),IMM,MSG('Browsing the PalletVerloop file'),#FIELDS(Pal3:FirmaNaam, |
            Pal3:Transporteur,LOC:PalletSoort,Pal3:Planning_DATE,Pal3:Planning_TIME,Pal3:Inkomend, |
            Pal3:Uitgaand,LOC:Saldo,LOC:KlantSaldoIn,LOC:Firmanaam,LOC:MatchingMutatie,LOC:HandmatigMutatie, |
            Pal3:Opmerking,Pal:PalletMutatieID,Pal3:PalletSoortID),#ORIG(?List),#SEQ(1),#ORDINAL(19)
