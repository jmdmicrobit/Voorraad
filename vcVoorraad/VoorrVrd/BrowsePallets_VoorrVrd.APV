                'Firmanaam~C(0)@s50@'),FROM(Queue:FileDropCombo),IMM,#FIELDS(Rel:FirmaNaam), |
                #ORIG(Rel:RelatieID),#SEQ(3),#ORDINAL(3)
              PROMPT('Klant:'),AT(13,47),USE(?PROMPT1),#ORDINAL(4)
              BUTTON('Excel'),AT(12,250,55,15),USE(?Excel:1),LEFT,ICON('Excel.ico'),#ORDINAL(5)
              BUTTON('...'),AT(265,45,22,14),USE(?Lookup:1),HIDE,#ORDINAL(6)
            END
            TAB('&2) Per leverancier'),USE(?TAB2),#ORDINAL(7)
              COMBO(@s100),AT(59,47,196,10),USE(AARel:FirmaNaam),DROP(25),FORMAT('200L(2)|M~' & |
                'Firmanaam~C(0)@s50@'),FROM(Queue:FileDropCombo:2),#FIELDS(AARel:FirmaNaam), |
                #ORIG(AARel:FirmaNaam),#SEQ(6),#ORDINAL(8)
              STRING('Leverancier:'),AT(15,47),USE(?STRING2),#ORDINAL(9)
              BUTTON('...'),AT(261,45,21),USE(?Lookup:2),HIDE,#ORDINAL(10)
              BUTTON('Excel'),AT(13,249,55,15),USE(?Excel:2),LEFT,ICON('Excel.ico'),#ORDINAL(11)
            END
            TAB('&3) Per Transporteur'),USE(?TAB1),#ORDINAL(12)
              PROMPT('Transporteur:'),AT(12,46),USE(?PROMPT2),#ORDINAL(13)
              COMBO(@s50),AT(61,46,207,10),USE(VTRA:FirmaNaam),DROP(25),FORMAT('200L(2)|M~' & |
                'Firma Naam~C(0)@s50@400L(2)|M~Plaats~C(0)@s100@'),FROM(Queue:FileDropCombo:1), |
                IMM,#FIELDS(VTRA:FirmaNaam,VTRA:Plaats),#ORIG(VTRA:FirmaNaam),#SEQ(5),#ORDINAL(14)
              BUTTON('Excel'),AT(13,249,55,15),USE(?Excel:3),LEFT,ICON('Excel.ico'),#ORDINAL(15)
